{
  "name": "txt-ss",
  "displayName": "txt-ss",
  "description": "An extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.pasteScreenshotText"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.pasteScreenshotText",
        "title": "Paste Screenshot as Text"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0"
  },
  "dependencies": {
    "tesseract.js": "^6.0.1"
  }
}
